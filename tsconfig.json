{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
      "@types/*": ["src/types/*"],
      "@content/*": ["src/content/*"]
    },
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
